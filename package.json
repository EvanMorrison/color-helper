{
  "name": "off-color",
  "version": "1.1.2",
  "description": "utility for adjusting color values",
  "main": "index.js",
  "files": [
    "index.js",
    "cjs",
    "umd",
    "README.md",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/EvanMorrison/off-color.git"
  },
  "scripts": {
    "prebuild": "rimraf cjs umd",
    "build": "npm-run-all --parallel build:*",
    "build:dev": "webpack --output-filename index.js --mode development",
    "build:prod": "cross-env NODE_ENV=production webpack --output-filename index.min.js --mode production"
  },
  "keywords": [
    "color",
    "shift",
    "change",
    "css"
  ],
  "author": "Evan Morrison <evanrmorrison@gmail.com> (https://evmorrison.com)",
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.6.5"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-minify": "0.5.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
